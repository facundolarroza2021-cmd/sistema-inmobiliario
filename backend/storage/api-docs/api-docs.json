                "responses": {
                    "200": {
                        "description": "Detalles de la propiedad"
                    },
                    "404": {
                        "description": "Propiedad no encontrada"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            },
            "delete": {
                "tags": [
