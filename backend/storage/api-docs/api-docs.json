                    "200": {
                        "description": "Lista de propiedades recuperada con éxito",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "direccion": {
                                                "type": "string",
                                                "example": "Av. Siempre Viva 742"
                                            },
                                            "tipo": {
                                                "type": "string",
                                                "example": "Casa"
                                            },
                                            "estado": {
                                                "type": "string",
                                                "example": "Disponible"
                                            },
                                            "precio_alquiler": {
                                                "type": "number",
                                                "example": 150000
                                            },
                                            "propietario": {
                                                "description": "Datos del dueño",
                                                "type": "object"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "Propiedades"
                ],
                "summary": "Crear una nueva propiedad",
                "operationId": "8f14f04fae02e4fe798f67893120cbf3",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "propietario_id",
                                    "direccion",
                                    "tipo"
                                ],
                                "properties": {
                                    "propietario_id": {
                                        "description": "ID del dueño existente",
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "direccion": {
                                        "type": "string",
                                        "example": "Av. Siempre Viva 742"
                                    },
                                    "tipo": {
                                        "type": "string",
                                        "example": "Departamento"
                                    },
                                    "ambientes": {
                                        "type": "integer",
                                        "example": 3
                                    },
                                    "precio_alquiler": {
                                        "description": "Opcional",
                                        "type": "number",
                                        "example": null
                                    },
                                    "comision": {
                                        "description": "Opcional",
                                        "type": "number",
                                        "example": null
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Propiedad creada con éxito",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Propiedad creada"
                                        },
                                        "data": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/propiedades/{id}": {
