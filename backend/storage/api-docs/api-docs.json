                ],
                "summary": "Registrar un nuevo inquilino",
                "operationId": "0feb896b9f949b81450b32e6088f079b",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "nombre_completo",
                                    "dni"
                                ],
                                "properties": {
                                    "nombre_completo": {
                                        "type": "string",
                                        "example": "Carlos Pérez"
                                    },
                                    "dni": {
                                        "type": "string",
                                        "example": "30123456"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "carlos@mail.com"
                                    },
                                    "telefono": {
                                        "type": "string",
                                        "example": "3454123456"
                                    },
                                    "garante_nombre": {
                                        "type": "string",
                                        "example": "Padre de Carlos"
                                    },
                                    "garante_dni": {
                                        "type": "string",
                                        "example": "20123456"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Inquilino creado"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/inquilinos/{id}": {
            "put": {
                "tags": [
                    "Inquilinos"
