        "/api/propiedades/{id}/fotos": {
            "post": {
                "tags": [
                    "Propiedades"
                ],
                "summary": "Subir una foto a la propiedad",
                "operationId": "7c50385197f4f61f32feb4e1414393fb",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID de la propiedad",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "description": "Archivo de imagen",
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "properties": {
                                    "imagen": {
                                        "description": "Archivo de imagen (jpg, png, jpeg)",
                                        "type": "string",
                                        "format": "binary"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Foto subida con éxito",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "mensaje": {
                                            "type": "string",
                                            "example": "Foto subida"
                                        },
                                        "url": {
                                            "type": "string",
                                            "example": "http://localhost:8000/storage/propiedades/1/foto.jpg"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Error de validación (archivo muy grande o formato inválido)"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
